{
    "type": "project",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "craue/formflow-bundle": "^3.6",
        "dg/mysql-dump": "^1.5",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6.10|^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.5.11",
        "drenso/symfony-oidc-bundle": "^2.0",
        "eluceo/ical": "^2.0",
        "overblog/graphql-bundle": "^0.15",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.18",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/runtime": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/web-link": "^5.0",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.0",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/twig": "^2.11",
        "vich/uploader-bundle": "^1.10"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "ext-xdebug": "*",
        "dama/doctrine-test-bundle": "^6.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.4",
        "hautelook/alice-bundle": "^2.9",
        "liip/test-fixtures-bundle": "^2.0.0",
        "overblog/graphiql-bundle": "^0.2.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-doctrine": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/debug-bundle": "^5.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.0",
        "symfony/var-dumper": "^5.0",
        "symfony/web-profiler-bundle": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\Helper\\": "tests/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "php .hooks/install.php",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "php bin/phpunit --testdox"
        ],
        "lint": [
            "php-cs-fixer fix src --verbose --dry-run --show-progress=none"
        ],
        "fix": [
            "php-cs-fixer fix src"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse --memory-limit 2G"
        ],
        "db-rebuild": [
            "php bin/console doctrine:database:drop --force --if-exists",
            "php bin/console doctrine:database:create",
            "php bin/console doctrine:migrations:migrate -n"
        ],
        "db-rebuild-dev": [
            "@db-rebuild",
            "php bin/console app:create-account user@kiwi.nl user user",
            "php bin/console app:create-account --admin admin@kiwi.nl admin admin"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        },
        "hooks": {
            "pre-commit": [
                ".hooks/pre-commit"
            ]
        }
    }
}
